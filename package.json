        "daisyui": "^2.15.2"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.1",
        "autoprefixer": "^10.4.4",
        "concurrently": "^7.0.0",
        "daisyui": "^2.31.0",
